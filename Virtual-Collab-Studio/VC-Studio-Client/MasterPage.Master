    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/vanilla-tilt/1.7.0/vanilla-tilt.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/monaco-editor/0.34.1/min/vs/loader.min.js"></script>
    <script src="assets/js/index.js" defer></script>
    <script>
        var typingTimer;
        var doneTypingInterval = 1000; // 1-second delay

        function redirectToBlogs() {
            var title = document.getElementById("title").value.trim(); // Trim whitespace
            clearTimeout(typingTimer);

            if (title.length > 0) {
                // Fetch suggestions while the user is typing
                fetchSuggestions(title);

                typingTimer = setTimeout(function () {
                    var currentUrl = window.location.href;
                    var urlParams = new URLSearchParams(window.location.search);
                    var currentSearch = urlParams.get("search") || "";

                    // Check if already on blog.aspx and search query matches
                    if (currentUrl.includes("blog.aspx") && decodeURIComponent(currentSearch) === title) {
                        // Do nothing if already on the page with the same search query
                        return;
                    }

                    // Redirect only if the search term is different
                    window.location.href = "blog.aspx?search=" + encodeURIComponent(title);
                }, doneTypingInterval);
            }
        }

        function fetchSuggestions(query) {
            // Make an AJAX request to fetch suggestions
            fetch("blog.aspx/GetSuggestions", {
                method: "POST",
                headers: {
                    "Content-Type": "application/json"
                },
                body: JSON.stringify({ query: query })
            })
                .then(response => response.json())
                .then(data => {
                    displaySuggestions(data.d);
                })
                .catch(error => console.error("Error fetching suggestions:", error));
        }

        function displaySuggestions(suggestions) {
            var suggestionBox = document.getElementById("suggestion-box");
            suggestionBox.innerHTML = ""; // Clear previous suggestions

            suggestions.forEach(function (suggestion) {
                var suggestionItem = document.createElement("div");
                suggestionItem.className = "suggestion-item";
                suggestionItem.textContent = suggestion;

                // Add click event to select a suggestion
                suggestionItem.addEventListener("click", function () {
                    document.getElementById("title").value = suggestion;
                    suggestionBox.innerHTML = ""; // Clear suggestions
                });

                suggestionBox.appendChild(suggestionItem);
            });
        }

        window.onload = function () {
            var searchParam = new URLSearchParams(window.location.search).get("search");
            if (searchParam) {
                document.getElementById("title").value = decodeURIComponent(searchParam);
            }
        };

        // Add event listener for "/"
        document.addEventListener("keydown", function (event) {
            if (event.key === "/") {
                event.preventDefault();
                document.getElementById("title").focus();
            }
        });
    </script>
